public class Product {
	
	public int[] productExceptSelf(int[] nums) {
        
		if(nums.length<=1)
			return nums;
		
		int n = nums.length;
		int output[] = new int[n];
		output[0] = 1;
		
		for(int i=1;i<n;i++)
		{
			output[i] = output[i-1]*nums[i-1]; 
		}
		
		int temp = 1;
		
		for(int i=n-1;i>=0;i--)
		{
			output[i] = output[i] * temp;
			temp *= nums[i];
		}
		
		return output;
    }
}
